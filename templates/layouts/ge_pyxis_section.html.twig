<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="../../static/assets/img/favicon.png" rel="icon">
  <link href="../../static/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  {% do pimcore_head_link().appendStylesheet('https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,700,700i&display=swap') %}
  {% do pimcore_head_link().appendStylesheet('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css') %}

  <!-- Vendor CSS Files -->
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/vendor/animate.css/animate.min.css')) %}
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/vendor/bootstrap/css/bootstrap.min.css')) %}
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/vendor/aos/aos.css')) %}
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/vendor/bootstrap-icons/bootstrap-icons.css')) %}
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/vendor/boxicons/css/boxicons.min.css')) %}
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/vendor/glightbox/css/glightbox.min.css')) %}
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/vendor/swiper/swiper-bundle.min.css')) %}
  {% if editmode %}
    {% do pimcore_head_link().appendStylesheet(asset('static/css/editmode.css')) %}
  {% endif %}

  <!-- Template Main CSS File -->
  {% do pimcore_head_link().appendStylesheet(asset('static/assets/css/style.css')) %}
  {{ pimcore_head_link() }}

  {% if document is instanceof('\\Pimcore\\Model\\Document\\Page') %}
      {% if document.getTitle() is not empty %}
          {% do pimcore_head_title().set(document.getTitle()) %}
      {% endif %}

      {% if document.getDescription() is not empty %}
          {% do pimcore_head_meta().setDescription(document.getDescription()) %}
      {% endif %}
      
      {% do pimcore_head_title().setSeparator(' ') %}
  {% endif %}
  {{ pimcore_head_title() }}

  <style>
@import url("https://fonts.googleapis.com/css2?family=Poppins:weight@100;200;300;400;500;600;700;800&display=swap");
    body{
    background-color:#fff;
    font-family: "Poppins", sans-serif;
    font-weight: 300;
    }

    .height{
    height: 100vh;
    }

    .app-button{

    width: 240px;
    height: 58px;
    border-radius: 10px;
    }

    .app-button span{

    font-size: 18px;
    }

    .buttons{

    display: grid;
    }
  </style>

  <!-- =======================================================
  * Template Name: Moderna - v4.11.0
  * Template URL: https://bootstrapmade.com/free-bootstrap-template-corporate-moderna/
  * Author: BootstrapMade.com
  * License: https://bootstrapmade.com/license/
  ======================================================== -->
</head>

<body>

  <!-- ======= Header ======= -->
  {% include "gepyxis/includes/header.html.twig" %}
  <!-- End Header -->

  <main id="main">
    <!-- ======= Our Projects Section ======= -->
    {% include "gepyxis/includes/breadcrumbs.html.twig" %}
    <!-- End Our Projects Section -->
    
    {{ block('content') }}

  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  {{ pimcore_inc(document.getProperty('footerGP')) }}

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <script>
    // Obtener el elemento select y el formulario
    var select = document.getElementById("selectCategory");
    var form = document.getElementById("searchSection");
    
    // Agregar un evento change al elemento select
    select.addEventListener("change", function() {
      // Enviar el formulario cuando se selecciona una opci√≥n
      form.submit();
    });
  </script>
  <!-- Vendor JS Files -->
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/purecounter/purecounter_vanilla.js')) %}
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/aos/aos.js')) %}
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/bootstrap/js/bootstrap.bundle.min.js')) %}
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/glightbox/js/glightbox.min.js')) %}
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/isotope-layout/isotope.pkgd.min.js')) %}
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/swiper/swiper-bundle.min.js')) %}
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/waypoints/noframework.waypoints.js')) %}
  {% do pimcore_head_script().appendFile(asset('static/assets/vendor/php-email-form/validate.js')) %}
  {% do pimcore_head_script().appendFile('https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js') %}

  {% do pimcore_head_script().appendFile('https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js',null,null,{ integrity:"sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q",crossorigin:"anonymous"}) %}
  {% do pimcore_head_script().appendFile('https://code.jquery.com/jquery-3.3.1.slim.min.js',null,null,{ integrity:"sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo",crossorigin:"anonymous"}) %}
  		
  <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBhOdIF3Y9382fqJYt5I_sswSrEw5eihAA"></script>
  
  <!-- Template Main JS File -->
  {% do pimcore_head_script().appendFile(asset('static/assets/js/main.js')) %}
  {% if not editmode %}
    {% do pimcore_head_script().appendFile(asset('static/js/frontend.js')) %}
  {% endif %}
  {{ pimcore_head_script() }}

</body>

</html>